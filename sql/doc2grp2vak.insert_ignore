cat bla.csv | sort | uniq| sed "s/\(.*\),\(.*\),\(.*\)/INSERT IGNORE INTO doc2grp2vak ( doc_id, grp2vak_id ) VALUES ( (SELECT id FROM ovckb.doc WHERE afkorting = '\3' ), ( SELECT grp2vak.id FROM grp2vak JOIN grp ON grp.id = grp2vak.grp_id JOIN vak ON vak.id = grp2vak.vak_id WHERE vak.afkorting = '\2' AND grp.naam = '\1' AND grp.schooljaar = '0910' ) );/" | mysql -p -u root -D ovckb
